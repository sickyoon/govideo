{
    "root": true,
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": [
        "airbnb"
    ],
    "rules": {
        "no-console": 0,
        "func-names": ["error", "never"],
        "class-methods-use-this": 0
    }
}